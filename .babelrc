{
  "plugins": [
    [
      "wrap-in-js",
      {
        "extensions": ["css$", "scss$"]
      }
    ],
    [
      "module-resolver", {
        "root": ["."],
        "alias": {
          "styles": "./styles"
        },
        "cwd": "babelrc"
    }],
    ["lodash"],
    [
      "import", {
        "libraryName": "antd"
      }
    ]
  ],
  "presets": [
    "react",
    "./babel-preset",
    "stage-0"
  ],
  "ignore": []
}
